<stack class="stack-parent" onclick="update">
    <progress type="arc" class="min-progress" percent="{{ percentage }}"/>

    <div class="container">
        <text class="title1">
            {{ title }}
        </text>

        <text class="time">
            {{ time }}
        </text>
    </div>

    <input type="button" class="prevPage" onclick="prevPage" value="上一页"/>
</stack>